  {
    type    compressible::CFDHAMsolidMoistureCoupledImpermeable;
    value    uniform -1e7;
  }
  terrain_sides
  {
    type    zeroGradient;
  }
  terrain_bottom
  {
    type    fixedValue;
    value    uniform -7e+07;
  }
}
