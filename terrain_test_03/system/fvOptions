FoamFile
{
  version    2.0;
  format    ascii;
  class    dictionary;
  location    "system";
  object    fvOptions;
}
residuals_air
{
  type    residuals;
  libs    ("libutilityFunctionObjects.so");
  region    air;
  fields    (alphat epsilon omega gcr k nut p p_rgh T TMean TI Ts rho rs qr qs U UMean w ws);
  writeControl    timeStep;
  writeInterval    1;
}
